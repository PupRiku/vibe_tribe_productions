{
  "kind": "collectionType",
  "collectionName": "games",
  "info": {
    "singularName": "game",
    "pluralName": "games",
    "displayName": "Game",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "system": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "season1": {
      "type": "boolean",
      "required": true
    },
    "season2": {
      "type": "boolean",
      "required": true
    },
    "spotifyID": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "gameMaster": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::person.person",
      "inversedBy": "gm_of"
    },
    "players": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::person.person",
      "inversedBy": "player_in"
    }
  }
}
