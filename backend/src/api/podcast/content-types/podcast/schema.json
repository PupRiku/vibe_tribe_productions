    "description": {
      "type": "text",
      "required": true
    },
    "seasons": {
      "type": "integer",
      "required": true
    },
    "spotify": {
      "type": "string"
    },
    "google": {
      "type": "string"
    },
    "apple": {
      "type": "string"
    },
    "audible": {
      "type": "string"
    },
    "youtube": {
      "type": "string"
    },
    "people": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::person.person",
      "inversedBy": "podcasts"
    }
  }
}
